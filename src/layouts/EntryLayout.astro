---
import Link from "../components/Link.astro";
import Layout from "../layouts/Layout.astro";

interface Props {
  title: string;
  count: number;
  description: string;
}

const { title, count, description } = Astro.props;
---

<Layout title={title} count={count} description={description}>
  <br />
  <section id="content">
    <slot />
  </section>
</Layout>
<style>
  #content {
    width: 40%;
    margin: auto;
  }
</style>
